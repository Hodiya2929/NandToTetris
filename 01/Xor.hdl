/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
  
	Not(in=a, out=notA);
	And(a=notA, b=b, out=bAndNotA);
	Not(in=b, out=notB);
	And(a=a, b=notB, out=aAndNotB);
	Or(a=bAndNotA, b=aAndNotB, out=out);
}